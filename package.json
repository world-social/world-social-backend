{
  "name": "worldsocial-backend",
  "version": "1.0.0",
  "description": "Backend for WorldSocial - A decentralized social media platform",
  "main": "src/app.js",
  "scripts": {
    "start": "pnpm install && npx prisma generate && npx prisma migrate deploy && node src/app.js",
    "dev": "nodemon src/app.js",
    "test": "jest",
    "lint": "eslint .",
    "format": "prettier --write .",
    "compile": "hardhat compile",
    "test:contracts": "hardhat test",
    "deploy:local": "hardhat run src/contracts/deploy.js --network hardhat",
    "deploy:testnet": "hardhat run src/contracts/deploy.js --network sepolia",
    "deploy:mainnet": "hardhat run src/contracts/deploy.js --network mainnet",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "prisma:seed": "node prisma/seed.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.775.0",
    "@openzeppelin/contracts": "^5.0.1",
    "@prisma/client": "^5.10.2",
    "@worldcoin/minikit-js": "^1.0.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "ethers": "^6.13.5",
    "express": "^4.18.3",
    "express-rate-limit": "^7.5.0",
    "express-validator": "^7.0.1",
    "fluent-ffmpeg": "^2.1.2",
    "helmet": "^7.2.0",
    "jsonwebtoken": "^9.0.2",
    "minio": "^7.1.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "redis": "^4.7.0",
    "socket.io": "^4.7.4",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.11.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^4.0.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.19",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "hardhat": "^2.20.1",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "prettier-plugin-solidity": "^1.3.1",
    "prisma": "^5.10.2",
    "supertest": "^6.3.4"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  }
}
